from __future__ import annotations\
import math\
from typing import Callable, Dict\
\
PHI: float = (1.0 + 5 ** 0.5) / 2.0\
LN_PHI: float = math.log(PHI)\
\
def g_m(m: int) -> float:\
    # g_m = (-1)^(m+1) / (m \uc0\u966 ^m)\
    sign = 1 if ((m + 1) % 2 == 0) else -1\
    return sign * (1.0 / (m * (PHI ** m)))\
\
def _integrate_gamma_over_window(ln_m: float, gamma_func: Callable[[float], float], steps: int = 96) -> float:\
    a = ln_m; b = ln_m + LN_PHI\
    if steps % 2 == 1: steps += 1\
    h = (b - a) / steps\
    tot = 0.0\
    for i in range(steps + 1):\
        ln_mu = a + i*h\
        mu = math.exp(ln_mu)\
        w = 4.0 if i % 2 == 1 else 2.0\
        if i == 0 or i == steps: w = 1.0\
        tot += w * gamma_func(mu)\
    return (h/3.0) * tot\
\
def compute_f_i_local_cycle(ln_m: float, gamma_func: Callable[[float], float], invariant_by_m: Dict[int, float]) -> float:\
    integral = _integrate_gamma_over_window(ln_m, gamma_func)\
    gap = sum(g_m(m)*I for m, I in invariant_by_m.items())\
    return (integral / LN_PHI) + gap\
\
def compute_f_i(ln_m: float, gamma_func: Callable[[float], float], invariant_by_m: Dict[int, float]) -> float:\
    return compute_f_i_local_cycle(ln_m, gamma_func, invariant_by_m)\
\
def compute_f_i_phi_sheet(\
    ln_m: float,\
    gamma_func: Callable[[float], float],\
    invariant_by_m: Dict[int, float],\
    signed_weights: bool = False,\
    tail_tol: float = 1e-6,\
    max_K: int = 64,\
    steps_per_window: int = 96,\
) -> float:\
    # adaptive \uc0\u966 -sheet weights from g_\{k+1\}\
    raw = []\
    for k in range(max_K):\
        raw.append(g_m(k+1) if signed_weights else abs(g_m(k+1)))\
        tail = sum(abs(g_m(j+1)) for j in range(k+1, max_K))\
        if tail < tail_tol:\
            K = k+1\
            break\
    else:\
        K = max_K\
    L1 = sum(abs(x) for x in raw[:K]) or 1.0\
    w = [x / L1 for x in raw[:K]]\
\
    integ = 0.0\
    for k in range(K):\
        def gamma_shift(mu, k=k): return gamma_func(mu * (PHI**k))\
        integ += w[k] * _integrate_gamma_over_window(ln_m, gamma_shift, steps=steps_per_window)\
\
    gap = sum(g_m(m)*I for m, I in invariant_by_m.items())\
    return (integ / LN_PHI) + gap\
\
def solve_mass_fixed_point_phi_sheet(\
    B_i: float, E_coh: float, r_i: int,\
    gamma_func: Callable[[float], float],\
    invariant_by_m: Dict[int, float],\
    ln_m_init: float,\
    tol: float = 1e-12, max_iter: int = 240,\
    **phi_sheet_kwargs,\
) -> float:\
    ln_BE = math.log(B_i*E_coh)\
    ln_m = ln_m_init\
    for _ in range(max_iter):\
        f_i = compute_f_i_phi_sheet(ln_m, gamma_func, invariant_by_m, **phi_sheet_kwargs)\
        ln_new = ln_BE + r_i*LN_PHI + f_i*LN_PHI\
        if not math.isfinite(ln_new):\
            ln_new = 0.5*(ln_m + ln_BE + r_i*LN_PHI)\
        if abs(ln_new - ln_m) < tol:\
            return ln_new\
        ln_m = 0.5*ln_m + 0.5*ln_new\
    return ln_m\
\
def solve_mass_fixed_point(B_i, E_coh, r_i, gamma_func, invariant_by_m, ln_m_init, tol: float = 1e-12, max_iter: int = 240):\
    # Back-compat: local cycle\
    ln_BE = math.log(B_i*E_coh)\
    ln_m = ln_m_init\
    for _ in range(max_iter):\
        f_i = compute_f_i_local_cycle(ln_m, gamma_func, invariant_by_m)\
        ln_new = ln_BE + r_i*LN_PHI + f_i*LN_PHI\
        if not math.isfinite(ln_new): ln_new = 0.5*(ln_m + ln_BE + r_i*LN_PHI)\
        if abs(ln_new - ln_m) < tol: return ln_new\
        ln_m = 0.5*ln_m + 0.5*ln_new\
    return ln_m\
\
def mass_dimensionless(B_i: float, E_coh: float, r_i: int, f_i: float) -> float:\
    return B_i * E_coh * (PHI ** (r_i + f_i))\
""")\
